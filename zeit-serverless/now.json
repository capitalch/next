{
	"version": 2,
	"name": "zeit-serverless",
	"builds": [
		{
			"src": "next.config.js",
			"use": "@now/next"
		},
		{
			"src": "/lambdas/func2.js",
			"use": "@now/node"
		},
		{
			"src": "/lambdas/lcontacts.js",
			"use": "@now/node"
		},
		{
			"src": "/lambdas/lexpress.js",
			"use": "@now/node"
		}
	],
	"routes": [
		{
			"src":"/contacts",
			"dest":"/lambdas/lcontacts.js"
		},
		{
			"src":"/users",
			"dest":"/lambdas/lcontacts.js"
		},
		{
			"src":"/posts",
			"dest":"/lambdas/lexpress.js"
		},
		{
			"src":"/func2",
			"dest":"/lambdas/func2.js"
		},
		{
			
			"src":"/test",
			"dest":"/lambdas/lexpress.js"
		}
	]
}