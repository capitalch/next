{
	"version": 2,
	"name": "zeit-serverless-function",
	"builds": [
		{
			"src": "package.json",
			"use": "@now/next"
		},
		{
			"src": "*.js",
			"use": "@now/node"
		},
		{
			"src": "/lambdas/*.js",
			"use": "@now/node"
		}
	],
	"routes": [
		{
			"src": "/a",
			"dest": "/func1.js"
		},
		{
			"src": "/b",
			"dest": "/lambdas/func2.js"
		},
		{
			"src": "/clientposts",
			"dest": "/lambdas/posts.js"
		},
		{
			"src": "/posts",
			"dest": "/server.js"
		}
	]
}